{
    "headers": [
        {
            "source": "/app/(.*)\\.apk",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/vnd.android.package-archive"
                }
            ],
            "crons": [
                {
                    "path": "/api/keep-alive",
                    "schedule": "*/5 * * * *"
                }
            ]
        }
    ]
}